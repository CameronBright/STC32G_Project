  282   1        q3 = q3 + ( q0*gz + q1*gy - q2*gx) * halfT;
  283   1      
  284   1        norm = sqrt(q0*q0 + q1*q1 + q2*q2 + q3*q3);
  285   1        q0 = q0 / norm;
  286   1        q1 = q1 / norm;
  287   1        q2 = q2 / norm;
  288   1        q3 = q3 / norm;
