  248          void IMUupdate(float gx, float gy, float gz, float ax, float ay, float az)
  249          {
  250   1        float data norm;
  251   1        float idata vx, vy, vz;
  252   1        float idata ex, ey, ez;
  253   1      
  254   1        norm = sqrt(ax*ax + ay*ay + az*az); //把加速度计的三维向量转成单维向量   
  255   1        ax = ax / norm;
