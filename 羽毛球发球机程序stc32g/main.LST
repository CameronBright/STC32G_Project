  271   1        exInt = exInt + ex * Ki;
  272   1        eyInt = eyInt + ey * Ki;
  273   1        ezInt = ezInt + ez * Ki;
  274   1      
  275   1        gx = gx + Kp*ex + exInt;
  276   1        gy = gy + Kp*ey + eyInt;
  277   1        gz = gz + Kp*ez + ezInt;
